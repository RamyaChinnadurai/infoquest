import java.util.*;
public class Factorial {
	public static void main(String[] args) {
		int num,fact=1;
		System.out.println("Enter a number");
		Scanner in = new Scanner(System.in);
		num = in.nextInt();
		for(int i=1;i<=num;i++){
			fact = fact * i;
		}
       System.out.println("The factorial of the given no is " + fact) ;
	}
}
